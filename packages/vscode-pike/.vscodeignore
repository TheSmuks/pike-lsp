# VSIX ignore file
# Add files/directories here to exclude them from the VSIX package

# Source files (we only need compiled dist)
src/
tsconfig.tsbuildinfo

# Development files
.vscode-test/
.vscode/

# Image source (SVG not needed, PNG is used)
images/icon.svg

# Tests
**/*.test.ts
**/*.spec.ts
test/

# NPM/PNPM lockfiles
package-lock.json
pnpm-lock.yaml

# Git
.git/
.gitignore

# CI/CD
.github/

# Build scripts (not needed in VSIX)
scripts/

# Server source maps (optional, remove for smaller package)
server/**/*.map
